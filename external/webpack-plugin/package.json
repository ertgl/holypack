{
  "name": "@holypack/webpack-plugin",
  "keywords": [
    "webpack",
    "webpack-plugin",
    "holypack",
    "holypack-external"
  ],
  "scripts": {
    "build": "yarn clean && yarn babel:build::cjs && yarn babel:build::esm && yarn tsc:build::types && yarn hack:update-package-exports",
    "clean": "rm -fr dist"
  },
  "devDependencies": {
    "@holypack/core": "workspace:^",
    "@holypack/integration-webpack": "workspace:^",
    "webpack": "^5.99.7"
  },
  "peerDependencies": {
    "@holypack/core": "*",
    "@holypack/integration-webpack": "*",
    "webpack": "*"
  },
  "files": [
    "./dist/",
    "./src/",
    "./package.json"
  ],
  "type": "module",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./src/index.ts"
    },
    "./config/definition": {
      "types": "./dist/types/config/definition/index.d.ts",
      "import": "./dist/esm/config/definition/index.mjs",
      "require": "./dist/cjs/config/definition/index.cjs",
      "default": "./src/config/definition/index.ts"
    },
    "./config/definition/": {
      "types": "./dist/types/config/definition/index.d.ts",
      "import": "./dist/esm/config/definition/index.mjs",
      "require": "./dist/cjs/config/definition/index.cjs",
      "default": "./src/config/definition/index.ts"
    },
    "./config/definition/*.d.ts": {
      "types": "./dist/types/config/definition/*.d.ts",
      "default": "./src/config/definition/*.ts"
    },
    "./config/definition/*.cjs": {
      "types": "./dist/types/config/definition/*.d.ts",
      "import": "./dist/esm/config/definition/*.mjs",
      "require": "./dist/cjs/config/definition/*.cjs",
      "default": "./src/config/definition/*.ts"
    },
    "./config/definition/*.js": {
      "types": "./dist/types/config/definition/*.d.ts",
      "import": "./dist/esm/config/definition/*.mjs",
      "require": "./dist/cjs/config/definition/*.cjs",
      "default": "./src/config/definition/*.ts"
    },
    "./config/definition/*.mjs": {
      "types": "./dist/types/config/definition/*.d.ts",
      "import": "./dist/esm/config/definition/*.mjs",
      "require": "./dist/cjs/config/definition/*.cjs",
      "default": "./src/config/definition/*.ts"
    },
    "./config/definition/*.ts": {
      "types": "./dist/types/config/definition/*.d.ts",
      "import": "./dist/esm/config/definition/*.mjs",
      "require": "./dist/cjs/config/definition/*.cjs",
      "default": "./src/config/definition/*.ts"
    },
    "./config/definition/*": {
      "types": "./dist/types/config/definition/*",
      "import": "./dist/esm/config/definition/*",
      "require": "./dist/cjs/config/definition/*",
      "default": "./src/config/definition/*"
    },
    "./config": {
      "types": "./dist/types/config/index.d.ts",
      "import": "./dist/esm/config/index.mjs",
      "require": "./dist/cjs/config/index.cjs",
      "default": "./src/config/index.ts"
    },
    "./config/": {
      "types": "./dist/types/config/index.d.ts",
      "import": "./dist/esm/config/index.mjs",
      "require": "./dist/cjs/config/index.cjs",
      "default": "./src/config/index.ts"
    },
    "./config/*.d.ts": {
      "types": "./dist/types/config/*.d.ts",
      "default": "./src/config/*.ts"
    },
    "./config/*.cjs": {
      "types": "./dist/types/config/*.d.ts",
      "import": "./dist/esm/config/*.mjs",
      "require": "./dist/cjs/config/*.cjs",
      "default": "./src/config/*.ts"
    },
    "./config/*.js": {
      "types": "./dist/types/config/*.d.ts",
      "import": "./dist/esm/config/*.mjs",
      "require": "./dist/cjs/config/*.cjs",
      "default": "./src/config/*.ts"
    },
    "./config/*.mjs": {
      "types": "./dist/types/config/*.d.ts",
      "import": "./dist/esm/config/*.mjs",
      "require": "./dist/cjs/config/*.cjs",
      "default": "./src/config/*.ts"
    },
    "./config/*.ts": {
      "types": "./dist/types/config/*.d.ts",
      "import": "./dist/esm/config/*.mjs",
      "require": "./dist/cjs/config/*.cjs",
      "default": "./src/config/*.ts"
    },
    "./config/*": {
      "types": "./dist/types/config/*",
      "import": "./dist/esm/config/*",
      "require": "./dist/cjs/config/*",
      "default": "./src/config/*"
    },
    "./*.d.ts": {
      "types": "./dist/types/*.d.ts",
      "default": "./src/*.ts"
    },
    "./*.cjs": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/esm/*.mjs",
      "require": "./dist/cjs/*.cjs",
      "default": "./src/*.ts"
    },
    "./*.js": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/esm/*.mjs",
      "require": "./dist/cjs/*.cjs",
      "default": "./src/*.ts"
    },
    "./*.mjs": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/esm/*.mjs",
      "require": "./dist/cjs/*.cjs",
      "default": "./src/*.ts"
    },
    "./*.ts": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/esm/*.mjs",
      "require": "./dist/cjs/*.cjs",
      "default": "./src/*.ts"
    },
    "./*/": {
      "types": "./dist/types/*/index.d.ts",
      "import": "./dist/esm/*/index.mjs",
      "require": "./dist/cjs/*/index.cjs",
      "default": "./src/*/index.ts"
    },
    "./*": {
      "types": "./dist/types/*",
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*",
      "default": "./src/*"
    }
  }
}
