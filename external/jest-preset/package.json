{
  "name": "@holypack/jest-preset",
  "keywords": [
    "jest",
    "jest-preset",
    "holypack",
    "holypack-external"
  ],
  "scripts": {
    "build": "yarn clean && yarn babel:build::cjs && yarn babel:build::esm && yarn tsc:build::types && yarn hack:update-package-exports",
    "clean": "rm -fr dist"
  },
  "devDependencies": {
    "@holypack/core": "workspace:^",
    "@holypack/integration-jest": "workspace:^",
    "jest": "^29.7.0"
  },
  "peerDependencies": {
    "@holypack/core": "*",
    "@holypack/integration-jest": "*",
    "jest": "*"
  },
  "files": [
    "./dist/",
    "./src/",
    "./jest-preset.cjs",
    "./jest-preset.mjs",
    "./package.json"
  ],
  "type": "module",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exportsGenerator": {
    "prepend": {
      "./jest-preset": {
        "import": "./jest-preset.mjs",
        "default": "./jest-preset.mjs"
      }
    }
  },
  "exports": {
    "./jest-preset": {
      "import": "./jest-preset.mjs",
      "default": "./jest-preset.mjs"
    },
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./src/index.ts"
    },
    "./*.d.ts": {
      "types": "./dist/types/*.d.ts",
      "default": "./src/*.ts"
    },
    "./*.cjs": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/esm/*.mjs",
      "require": "./dist/cjs/*.cjs",
      "default": "./src/*.ts"
    },
    "./*.js": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/esm/*.mjs",
      "require": "./dist/cjs/*.cjs",
      "default": "./src/*.ts"
    },
    "./*.mjs": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/esm/*.mjs",
      "require": "./dist/cjs/*.cjs",
      "default": "./src/*.ts"
    },
    "./*.ts": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/esm/*.mjs",
      "require": "./dist/cjs/*.cjs",
      "default": "./src/*.ts"
    },
    "./*/": {
      "types": "./dist/types/*/index.d.ts",
      "import": "./dist/esm/*/index.mjs",
      "require": "./dist/cjs/*/index.cjs",
      "default": "./src/*/index.ts"
    },
    "./*": {
      "types": "./dist/types/*",
      "import": "./dist/esm/*",
      "require": "./dist/cjs/*",
      "default": "./src/*"
    }
  }
}
