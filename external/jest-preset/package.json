{
  "name": "@holypack/jest-preset",
  "keywords": [
    "jest",
    "jest-preset",
    "holypack",
    "holypack-external"
  ],
  "scripts": {
    "build": "yarn babel:build:clean && yarn babel:build::cjs && yarn babel:build::esm && yarn tsc:build::types && yarn export-map-generator:update",
    "build:clean": "yarn babel:build:clean",
    "clean": "yarn build:clean"
  },
  "devDependencies": {
    "@holypack/core": "workspace:^",
    "@holypack/integration-jest": "workspace:^",
    "jest": "^30.0.2"
  },
  "peerDependencies": {
    "@holypack/core": "*",
    "@holypack/integration-jest": "*",
    "jest": "*"
  },
  "files": [
    "./dist/",
    "./jest-preset.mjs",
    "./package.json"
  ],
  "type": "module",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "./default.cjs": {
      "types": "./dist/types/default.d.ts",
      "import": "./dist/esm/default.mjs",
      "require": "./dist/cjs/default.cjs",
      "default": "./src/default.ts"
    },
    "./default.d.ts": {
      "types": "./dist/types/default.d.ts",
      "default": "./src/default.ts"
    },
    "./default.js": {
      "types": "./dist/types/default.d.ts",
      "import": "./dist/esm/default.mjs",
      "require": "./dist/cjs/default.cjs",
      "default": "./src/default.ts"
    },
    "./default.mjs": {
      "types": "./dist/types/default.d.ts",
      "import": "./dist/esm/default.mjs",
      "require": "./dist/cjs/default.cjs",
      "default": "./src/default.ts"
    },
    "./default": {
      "types": "./dist/types/default.d.ts",
      "import": "./dist/esm/default.mjs",
      "require": "./dist/cjs/default.cjs",
      "default": "./src/default.ts"
    },
    "./index.cjs": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./src/index.ts"
    },
    "./index.d.ts": {
      "types": "./dist/types/index.d.ts",
      "default": "./src/index.ts"
    },
    "./index.js": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./src/index.ts"
    },
    "./index.mjs": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./src/index.ts"
    },
    "./index": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./src/index.ts"
    },
    "./jest-preset": {
      "import": "./jest-preset.mjs",
      "default": "./jest-preset.mjs"
    },
    "./package.json": {
      "default": "./package.json"
    },
    "./preset-options.cjs": {
      "types": "./dist/types/preset-options.d.ts",
      "import": "./dist/esm/preset-options.mjs",
      "require": "./dist/cjs/preset-options.cjs",
      "default": "./src/preset-options.ts"
    },
    "./preset-options.d.ts": {
      "types": "./dist/types/preset-options.d.ts",
      "default": "./src/preset-options.ts"
    },
    "./preset-options.js": {
      "types": "./dist/types/preset-options.d.ts",
      "import": "./dist/esm/preset-options.mjs",
      "require": "./dist/cjs/preset-options.cjs",
      "default": "./src/preset-options.ts"
    },
    "./preset-options.mjs": {
      "types": "./dist/types/preset-options.d.ts",
      "import": "./dist/esm/preset-options.mjs",
      "require": "./dist/cjs/preset-options.cjs",
      "default": "./src/preset-options.ts"
    },
    "./preset-options": {
      "types": "./dist/types/preset-options.d.ts",
      "import": "./dist/esm/preset-options.mjs",
      "require": "./dist/cjs/preset-options.cjs",
      "default": "./src/preset-options.ts"
    },
    "./preset.cjs": {
      "types": "./dist/types/preset.d.ts",
      "import": "./dist/esm/preset.mjs",
      "require": "./dist/cjs/preset.cjs",
      "default": "./src/preset.ts"
    },
    "./preset.d.ts": {
      "types": "./dist/types/preset.d.ts",
      "default": "./src/preset.ts"
    },
    "./preset.js": {
      "types": "./dist/types/preset.d.ts",
      "import": "./dist/esm/preset.mjs",
      "require": "./dist/cjs/preset.cjs",
      "default": "./src/preset.ts"
    },
    "./preset.mjs": {
      "types": "./dist/types/preset.d.ts",
      "import": "./dist/esm/preset.mjs",
      "require": "./dist/cjs/preset.cjs",
      "default": "./src/preset.ts"
    },
    "./preset": {
      "types": "./dist/types/preset.d.ts",
      "import": "./dist/esm/preset.mjs",
      "require": "./dist/cjs/preset.cjs",
      "default": "./src/preset.ts"
    },
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./src/index.ts"
    }
  }
}
