{
  "name": "@holypack/eslint-plugin",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "holypack",
    "holypack-external"
  ],
  "scripts": {
    "build": "yarn babel:build:clean && yarn babel:build::cjs && yarn babel:build::esm && yarn tsc:build::types && yarn export-map-generator:update",
    "build:clean": "yarn babel:build:clean",
    "clean": "yarn build:clean"
  },
  "devDependencies": {
    "@holypack/core": "workspace:^",
    "@holypack/integration-eslint": "workspace:^",
    "eslint": "^9.30.1"
  },
  "peerDependencies": {
    "@holypack/core": "*",
    "@holypack/integration-eslint": "*",
    "eslint": "*"
  },
  "files": [
    "./dist/",
    "./package.json"
  ],
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "./index.cjs": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./src/index.ts"
    },
    "./index.d.ts": {
      "types": "./dist/types/index.d.ts",
      "default": "./src/index.ts"
    },
    "./index.js": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./src/index.ts"
    },
    "./index.mjs": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./src/index.ts"
    },
    "./index": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./src/index.ts"
    },
    "./package.json": {
      "default": "./package.json"
    },
    "./plugin-options.cjs": {
      "types": "./dist/types/plugin-options.d.ts",
      "import": "./dist/esm/plugin-options.mjs",
      "require": "./dist/cjs/plugin-options.cjs",
      "default": "./src/plugin-options.ts"
    },
    "./plugin-options.d.ts": {
      "types": "./dist/types/plugin-options.d.ts",
      "default": "./src/plugin-options.ts"
    },
    "./plugin-options.js": {
      "types": "./dist/types/plugin-options.d.ts",
      "import": "./dist/esm/plugin-options.mjs",
      "require": "./dist/cjs/plugin-options.cjs",
      "default": "./src/plugin-options.ts"
    },
    "./plugin-options.mjs": {
      "types": "./dist/types/plugin-options.d.ts",
      "import": "./dist/esm/plugin-options.mjs",
      "require": "./dist/cjs/plugin-options.cjs",
      "default": "./src/plugin-options.ts"
    },
    "./plugin-options": {
      "types": "./dist/types/plugin-options.d.ts",
      "import": "./dist/esm/plugin-options.mjs",
      "require": "./dist/cjs/plugin-options.cjs",
      "default": "./src/plugin-options.ts"
    },
    "./plugin.cjs": {
      "types": "./dist/types/plugin.d.ts",
      "import": "./dist/esm/plugin.mjs",
      "require": "./dist/cjs/plugin.cjs",
      "default": "./src/plugin.ts"
    },
    "./plugin.d.ts": {
      "types": "./dist/types/plugin.d.ts",
      "default": "./src/plugin.ts"
    },
    "./plugin.js": {
      "types": "./dist/types/plugin.d.ts",
      "import": "./dist/esm/plugin.mjs",
      "require": "./dist/cjs/plugin.cjs",
      "default": "./src/plugin.ts"
    },
    "./plugin.mjs": {
      "types": "./dist/types/plugin.d.ts",
      "import": "./dist/esm/plugin.mjs",
      "require": "./dist/cjs/plugin.cjs",
      "default": "./src/plugin.ts"
    },
    "./plugin": {
      "types": "./dist/types/plugin.d.ts",
      "import": "./dist/esm/plugin.mjs",
      "require": "./dist/cjs/plugin.cjs",
      "default": "./src/plugin.ts"
    },
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./src/index.ts"
    }
  }
}
