    {
      "name": "Babel preset - CJS target (async)",
      "type": "node",
      "request": "launch",
      "internalConsoleOptions": "openOnSessionStart",
      "env": {
        "DEBUG": "true"
      },
      "runtimeArgs": [
        "--disable-warning=ExperimentalWarning",
        "--experimental-require-module",
        "--experimental-transform-types",
        "--experimental-vm-modules"
      ],
      "cwd": "${workspaceFolder}/external/babel-preset",
      "localRoot": "${workspaceFolder}/external/babel-preset/.build/debug",
      "remoteRoot": "${workspaceFolder}/external/babel-preset/src",
      "preLaunchTask": "npm: debug:build - external/babel-preset",
      "program": "${workspaceFolder}/external/babel-preset/src/default/async/cjs/BABEL_PRESET_HOLYPACK_ASYNC_CJS.ts",
      "stopOnEntry": true,
      "smartStep": true,
      "sourceMaps": true,
      "sourceMapRenames": true,
      "pauseForSourceMap": true,
      "outFiles": [
        "${workspaceFolder}/external/babel-preset/.build/debug/**/*.mjs"
      ],
      "skipFiles": [
        "<node_internals>/**",
        "node_modules/**"
      ]
    },
